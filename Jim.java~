import java.util.*;

/* hello */

class Main extends App {
  public static void main(String[] arguments) {
    SolPaint paint = new SolPaint();
    paint.run();
    ArrayList<Integer> foo = new ArrayList<>();
    foo.add(5);
    System.out.println(foo);
    int a = 0;
    int b = 1;
    int c = a + b;
    int d = a + b + c;
    System.out.println(d);
    d *= d;
    d *= d;
    System.out.println(d);
  }
}
